/**
 * used for storing a value that may be present
 *
 * Example:
 *
 * ```
 * let someValue = Has(15); // store a value, type is determined automatically
 * let someValue: Maybe<number> = HasNot; // no value (currently)
 * // to extract the value
 * when someValue {
 * 	Has(value) -> ...
 * 	HasNot -> ...
 * }
 * ```
 */
enum Maybe<|T|> {
	Has(T),
	HasNot,
}
